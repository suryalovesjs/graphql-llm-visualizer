{
    "name": "graphql-llm-visualizer",
    "version": "0.0.1",
    "description": "A TypeScript library that uses LLMs to analyze and visualize GraphQL services",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "bin": {
        "graphql-visualizer": "./dist/cli.js"
    },
    "scripts": {
        "build": "tsc",
        "start": "node dist/cli.js",
