        "typescript": "^5.3.2"
    },
    "devDependencies": {
        "@types/d3": "^7.4.3",
        "@types/graphql": "^14.5.0",
        "@types/inquirer": "^9.0.7",
        "@types/jest": "^29.5.10",
        "@types/node": "^20.10.0",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1"
    },
